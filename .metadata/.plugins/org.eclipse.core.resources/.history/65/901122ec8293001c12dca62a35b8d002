package com.pooja;

public class New {

	public static float addition(float n1, float n2) {
		return n1 + n2;
	}
	
	public static float subtration(float n1, float n2) {
		return n1 - n2;
	}
		
	public static float multipilcation(float n1, float n2) {
		return n1 * n2;
	}
		
	public static float division(float n1, float n2) {
		return n1 / n2;
	}
	
}
