package com.pooja;

import java.util.Scanner;

public class DoWhileLoop {
	
	public static void main(String args[]) {
		Scanner input = new Scanner(System.in);
		
		System.out.print("Enter number: ");
		int x = input.nextInt();
				
		do {
			System.out.println("X = " + x);
			//x= x + 3;
		} while(x ==1 || x ==2);

		}

}
