import java.util.HashMap;
import java.util.List;
import java.util.ArrayList;

public class Bookshelf {
	
	private HashMap<String, String[]> allBooks = new HashMap<>();
	
	
	
	public Bookshelf() {
		setItems();
	}
	
	 
	public void setItems() {
		int rand = (int) ((9 - 0 + 1) + 0);
    	String textId = "";
    	for(int i = 0; i < 10; i++) {
    		rand = (int) ((10 - 0 + 1) + 0);
    		textId = textId + String.valueOf(rand);
    	}
		allBooks.put("Fangirl", new String[] {"Rainbow Rowell", textId});
	}
	
	public void returnItem() {
