import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;

public class Catalog {
	
    private HashMap<Integer, String> items = new HashMap<>();
    private Bookshelf books;
    private List<String[]> tempList = new ArrayList<>();
     
    
    public Catalog() {
        books = new Bookshelf();
    }

    public void addItem() {
    	//books.addItem(title, bookInfo);
    }

    public void removeItem(String title) {
    	books.removeItem(title);
    }

      
    public void getItem(String title) {
    	if(items.containsKey(title)) {
    		String[] temp = {items.get(title)};
    		tempList.add(temp);
    	}
    	
    }
    
    public List returnItem() {
    	return tempList;
    }
    
    /*
    public static void main(String[] args) {
    	
    	Catalog cat = new Catalog();
    	
    	String[] arr = {"Rowling", "00000"};
    	
    	cat.addItem("Harry Potter", arr);
    	
    	System.out.println();
    	
    } 
    */
    
   
       
}


