//import java.util.ArrayList;
public class MultipleChoice {
	public static void main(String[] args) {
		
		String[][] magic = { {"James", "Sirius", "Remus", "Peter"}, {"Harry", "Ron", "Hermione"}	};
		
		for(int x = 0; x < magic.length; x++) {
			
			for(int y = 0; y < magic[x].length; y++) {
				System.out.println(magic[x][y]);
			}
			
			System.out.println("!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!");
		}
		
		
		
	}
	
	
	
	public static boolean mystery(int[] values) {
		
		for(int i = 0; i < values.length-1; i++) {
			if(Math.abs(values[i]-values[i+1]) > 15) {
				return false;
			}
		}
		return true;
	}
		
	public static void memberClass() {
		/*
		String[] memberNames = {"James","Lily","Sirius","Regulus","Remus","Peter"};
		ClubMembers members = new ClubMembers();
		members.addMembers(memberNames, 2023);
		members.removeMembers(2020);
		*/
	
	}
	
	
}
