package com.pooja;

public class Testing {
	int x = 5;
	
	public static void main(String[] args) {
		
		
		int x = 5;
		
		for(int i = 0; i <= x;i++) 
		{
			System.out.println("i = " + i );
			
			for(int y = 0; y <=3 ; y++) {
				
				System.out.println("y = " + y);
				
			}
			System.out.println("==============================");
		}
	}

}
