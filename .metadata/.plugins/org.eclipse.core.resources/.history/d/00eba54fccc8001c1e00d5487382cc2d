public class MultipleChoice {
	public static void main(String[] args) {
		
		String[][] magic = { {"James", "Sirius", "Remus", "Peter"}, {"Harry", "Ron", "Hermione"} };
		
		for(int r = 1; r < 5; r++) {
			System.out.println("*");

			for(int c = 0; c < r; c++) {
				System.out.print("*");
				//System.out.println(magic[r][c]);
			}
			
			//System.out.println("----------------");
		}
		
		
		
	}
	
	
	
	public static boolean mystery(int[] values) {
		
		for(int i = 0; i < values.length-1; i++) {
			if(Math.abs(values[i]-values[i+1]) > 15) {
				return false;
			}
		}
		return true;
	}
		
	public static void memberClass() {
		/*
