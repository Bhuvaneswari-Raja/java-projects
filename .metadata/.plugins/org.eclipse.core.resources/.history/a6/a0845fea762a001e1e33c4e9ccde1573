import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;

public class Catalog {
	
    private HashMap<String, String[]> items;
    private Bookshelf books;
     
    
    public Catalog() {
        books = new Bookshelf();
    }

    public void addItem(String title, String[] bookInfo) {
    	books.addItem(title, bookInfo);
    }

    public void removeItem(String title) {
    	books.removeItem(title);
    }

      
    public void getItem(String title) {
    	books.searchItem(title);
    	books.returnItem();
    	
    }
    
    public void returnItem(String title, String[] bookInfo) {
    	books.addItem(title, bookInfo);
    }
    
    
    public static void main(String[] args) {
    	
    	Catalog cat = new Catalog();
    	
    	String[] arr = {"Rowling", "00000"}
    	
    	cat.addItem("Harry Potter", arr);
    	
    }
    
   
       
}


